    "gulp-zip": "^4.2.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "semantic-commitlint": "^1.3.2",
    "semantic-release": "^15.13.3",
    "semver": "^5.6.0",
    "vinyl": "^2.2.0"
