{
  "name": "ui5-fontawesome",
  "version": "1.4.0",
  "description": "A Font Awesome plugin for UI5",
  "browser": "js/ui5-fontawesome.js",
  "scripts": {
    "commit": "git-cz",
    "lint:eslint:ci": "eslint . --ignore-path .gitignore --format junit --output-file .reports/eslint/report.xml",
    "lint:eslint:local": "eslint . --ignore-path .gitignore",
    "lint:eslint": "if is-ci; then npm run lint:eslint:ci; else npm run lint:eslint:local; fi",
    "lint:commitlint:ci": "commitlint -f v1.2.0 -t HEAD",
    "lint:commitlint:local": "commitlint -f origin/master -t HEAD",
    "lint:commitlint": "if is-ci; then npm run lint:commitlint:ci; else npm run lint:commitlint:local; fi",
    "lint": "npm-run-all --parallel --continue-on-error lint:eslint lint:commitlint",
    "test:lint": "npm run lint",
    "test": "npm-run-all --parallel --continue-on-error test:*",
    "prepare": "gulp rebuild"
  },
  "repository": "github:zypA13510/ui5-fontawesome",
  "keywords": [
    "ui5",
    "font",
    "awesome",
    "fontawesome",
    "openui5",
    "sapui5",
    "icon"
  ],
  "author": "Yuping Zuo <yuping.zuo+HxmTqd@outlook.com> (https://github.com/zypA13510)",
  "contributors": [
    "Font Awesome (https://fontawesome.com/)"
  ],
  "license": "(MIT AND OFL-1.1)",
  "bugs": {
    "url": "https://github.com/zypA13510/ui5-fontawesome/issues"
  },
  "homepage": "https://github.com/zypA13510/ui5-fontawesome#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.11.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/prompt": "^8.2.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/exec": "^3.3.6",
    "@semantic-release/git": "^7.0.16",
    "bluebird": "^3.5.5",
    "commitizen": "^4.0.3",
    "commitlint": "^8.2.0",
    "del": "^5.1.0",
    "eslint": "^6.4.0",
    "gulp": "^4.0.2",
    "gulp-decompress": "^3.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^5.0.0",
    "is-ci": "^2.0.0",
    "lodash.pickby": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "semantic-release": "^15.13.24",
    "semver": "^6.3.0",
    "vinyl": "^2.2.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "files": [
    "fontawesome-free/**/*",
    "js/**/*.js",
    "js/**/*.min.js.map",
    "metadata/*.json",
    "package.json",
    "CHANGELOG.md",
    "LICENSE.txt",
    "README.md"
  ]
}
